<odoo>
    <data>
        
        <template id="minicart">

            <!-- Minicart -->
            <div class="minicart-flyout-container flyout-content flyout-container js-minicart-flyout-container">

                <div class="checkout-flyout-inner-content" style="padding-bottom:200px;">

                    <section class="flyout-section">
                        <div class="js-minicart-not-empty">
                            <div class="minicart-flyout-wrapper">
                                <span class="sr-only flyout-sr-only" tabindex="0" aria-hidden="true"></span>

                                <button type="button" class="side--close js-close-flyout icon-close" tabindex="0">
                                    <span class="sr-only">Close flyout</span>
                                    <img src="/dkaroma/static/img/icons/x-dark.svg" width="24" height="24" class="d-inline-block align-baseline" alt="" loading="lazy" />
                                </button>


                                <div class="minicart-flyout-header flyout-header">
                                    <input type="hidden" value="" class="js-cart-quantity" tabindex="-1" />

                                    <div class="centered">
                                        <h3 class="flyout-title">
                                            <span class="lang-en">My shopping cart</span>
                                            <span class="lang-ch">我的購物車</span>
                                            <span class="lang-zh">我的购物车</span>
                                        </h3>

                                        <span class="flyout-title-overview js-selected-products">
                                            <!-- products count: 2 products -->
                                        </span>
                                    </div>
                                </div>


                                <div class="minicart-flyout-body flyout-body">

                                    <div class="top-fade"></div>

                                    <div class="wrapper--minicart__list">
                                        <!-- Cart Products START -->

                                        

                                        <div class="gwp-minicart-wrapper"></div>
                                    </div>
                                </div>

                            </div>
                        </div>


                        <div class="js-minicart-empty hide">
                            <div class="minicart-flyout-wrapper js-minicart-flyout-wrapper">
                                <span class="sr-only flyout-sr-only" tabindex="0" aria-hidden="true"></span>

                                <button type="button" class="side--close js-close-flyout icon-close" tabindex="0">
                                    <span class="sr-only">Close flyout</span>
                                    <img src="/dkaroma/static/img/icons/x-dark.svg" width="24" height="24" class="d-inline-block align-baseline" alt="" loading="lazy" />
                                </button>

                                <div class="minicart-flyout-header flyout-header js-flyout-header">
                                    <input type="hidden" value="" class="js-cart-quantity" tabindex="-1" />

                                    <div class="centered">
                                        <h3 class="flyout-title">
                                            <span class="lang-en">My shopping cart</span>
                                            <span class="lang-ch">我的購物車</span>
                                            <span class="lang-zh">我的购物车</span>
                                        </h3>


                                    </div>
                                </div>

                                <div class="mini-cart-empty">
                                    <div class="mini-cart-empty-text">
                                        Oh, it's empty! Let's fill it up with joy
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>

                    <div class="minicart-flyout-checkout js-minicart-flyout-checkout has-shadow hide">
                        <div class="total">
                            <div class="total-line">
                                <span class="total-label">
                                    <span class="lang-en">Order subtotal</span>
                                    <span class="lang-ch">訂單小計</span>
                                    <span class="lang-zh">订单小计</span>
                                    <div class="content-asset">
                                        <span class="free-delivery-message">
                                            <div class="content-asset">
                                                Delivery in 1-3 workdays.
                                            </div>
                                        </span>
                                    </div>
                                </span>

                                <span class="total-value js-total-price">
                                    
                                </span>
                            </div>
                        </div>

                        <form method="" action="/shop/cart" class="dk-form" novalidate="novalidate">
                            <div class="minicart-continueshopping">
                                <button type="button" class="btn btn--secondary js-side--close continue-shopping-btn" tabindex="0">
                                    <span class="lang-en">Continue shopping</span>
                                    <span class="lang-ch">繼續購物</span>
                                    <span class="lang-zh">继续购物</span>
                                </button>
                            </div>

                            <button type="submit" class="btn btn--submit" tabindex="0">
                                <span class="lang-en">Go to cart</span>
                                <span class="lang-ch">去购物车</span>
                                <span class="lang-zh">去购物车</span>
                            </button>
                        </form>
                    </div>
                </div>


            </div>

        </template>

    </data>
</odoo>